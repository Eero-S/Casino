package kasinoPackage

class Card(val valueHand: Int, val valueTable: Int, val suit: String, val name: String, val points: Int, val pic: Int) {

  override def toString = if (valueTable >= 10) suit.head.toLower + valueTable.toString() + " " + pic else suit.head.toLower + 0.toString() + valueTable.toString() + " " + pic

}